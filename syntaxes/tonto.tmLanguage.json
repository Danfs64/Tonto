{
  "name": "tonto",
  "scopeName": "source.tonto",
  "fileTypes": [
    ".tonto"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "name": "keyword.control.tonto",
      "match": "\\b(abstract|aggregation|bringsAbout|categorizer|category|characterization|class|collective|comparative|complete|componentOf|composition|creation|datatype|derivation|disjoint|enum|enumeration|event|externalDependence|general|genset|historicalDependence|historicalrole|historicalRole|historicalrolemixin|historicalRoleMixin|instantiation|kind|manifestation|material|mediation|memberOf|mixin|mode|module|participation|participational|phase|phasemixin|phaseMixin|quality|quantity|ref|relator|role|rolemixin|roleMixin|situation|specializes|specifics|subCollectionOf|subkind|subQuantityOf|termination|triggers|type)\\b"
    },
    {
      "name": "string.quoted.double.tonto",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string.quoted.single.tonto",
      "begin": "'",
      "end": "'"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.block.tonto",
          "begin": "/\\*",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.tonto"
            }
          },
          "end": "\\*/",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.tonto"
            }
          }
        },
        {
          "begin": "//",
          "beginCaptures": {
            "1": {
              "name": "punctuation.whitespace.comment.leading.tonto"
            }
          },
          "end": "(?=$)",
          "name": "comment.line.tonto"
        }
      ]
    }
  }
}